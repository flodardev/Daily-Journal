<%- include("partials/header") %>
<h1>Compose</h1>

<form method="post">
    <div class="form-group">
        <label for="composeTitle">Title</label>
        <input name="composeTitle" class="form-control" id="composeTitle" type="text" autocomplete="off">
    </div>
    <div class="form-group">
        <label for="composeContent">Content</label>
        <textarea name="composeContent" class="form-control" id="composeContent" rows="5"></textarea>
    </div>
    <button type="submit" class="btn btn-primary btn-sm">Publish</button>    
</form>

<%- include("partials/footer") %>